{
  "name": "uno-app",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development DEBUG=uno-app:* nodemon ./bin/www",
    "db:create:migration": "npx sequelize migration:generate --name ",
    "db:migrate": "npx sequelize db:migrate",
    "db:rollback": "npx sequelize db:migrate:undo",
    "postinstall": "npx sequelize db:migrate"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-handlebars": "^6.0.3",
    "express-mysql-session": "^2.1.7",
    "express-session": "^1.17.2",
    "handlebars": "^4.7.7",
    "http-errors": "~1.6.3",
    "jade": "^0.29.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "pg": "^8.0.1",
    "pg-promise": "^10.11.1",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  },
  "engines": {
    "npm": "~8.5.4",
    "node": "~17.7.1"
  }
}
